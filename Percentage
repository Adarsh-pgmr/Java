import java.util.Scanner;

public class Main {

	public static void main(String[] args) {	//	int y=7;

	//	int x=++y+8;

	     Scanner sc=new Scanner(System.in);

	 	System.out.println("Enter the mark:\n");

	 	int a=sc.nextInt();

	 	int b=sc.nextInt();

	 	int c=sc.nextInt();

	 	float perc=(float)(a+b+c)/3;

	 	System.out.println("The percentage is "+ perc);

	}

}
